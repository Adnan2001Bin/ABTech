{
  "version": 2,
  "builds": [
    {
      "src": "client/build/index.html",
      "dest": "/"
    },
    {
      "src": "server/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/index.js"
    },
    {
      "src": "/.*",
      "dest": "client/build/index.html"
    }
  ]
}