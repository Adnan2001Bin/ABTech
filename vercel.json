{
  "version": 2,
  "name": "ab-tech",
  "builds": [
    {
      "src": "client/build",
      "use": "@vercel/static"
    },
    {
      "src": "server/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/app.js"
    },
    {
      "src": "/.*",
      "dest": "client/build/index.html"
    }
  ]
}